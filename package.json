{
  "name": "GivEconomy",
  "version": "0.2.0",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint src/ --ext .ts",
    "codegen:kovan": "graph codegen subgraph.kovan.yaml",
    "codegen:mainnet": "graph codegen subgraph.mainnet.yaml",
    "codegen:xdai": "graph codegen subgraph.xdai.yaml",
    "codegen:xdai:develop": "graph codegen subgraph.xdai.develop.yaml",
    "codegen:develop": "yarn codegen:xdai && yarn codegen:kovan",
    "clean": "rm -r generated build",
    "build:kovan": "graph build subgraph.kovan.yaml",
    "build:mainnet": "graph build subgraph.mainnet.yaml",
    "build:xdai": "graph build subgraph.xdai.yaml",
    "build:xdai:develop": "graph build subgraph.xdai.develop.yaml",
    "build:develop": "yarn build:xdai:develop && yarn build:kovan",
    "deploy:kovan": "graph deploy --product hosted-service mohammadranjbarz/giv-economy-kovan subgraph.kovan.yaml",
    "deploy:kovan:develop": "graph deploy --product hosted-service mohammadranjbarz/giv-economy-kovan-develop subgraph.kovan.yaml",
    "deploy:xdai": "graph deploy --product hosted-service giveth/giveth-economy-xdai subgraph.xdai.yaml",
    "deploy:xdai:develop": "graph deploy --product hosted-service mohammadranjbarz/giv-economy-xdai-develop subgraph.xdai.develop.yaml",
    "deploy:mainnet": "graph deploy  --product hosted-service  giveth/giveth-economy-mainnet subgraph.mainnet.yaml",
    "deploy:xdai:deployment7": "graph deploy --product hosted-service mohammadranjbarz/giv-economy-xdai-deployment-seven subgraph.xdai.develop.yaml",
    "deploy:kovan:deployment7": "graph deploy --product hosted-service mohammadranjbarz/giv-economy-kovan-deployment-seven subgraph.kovan.yaml",
    "deploy:develop": "yarn deploy:xdai:develop && yarn deploy:kovan:develop",
    "create-local": "graph create --node http://localhost:8020/ mohammadranjbarz/gardenUnipool",
    "remove-local": "graph remove --node http://localhost:8020/ mohammadranjbarz/gardenUnipool",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 mohammadranjbarz/gardenUnipool",
    "deploy:mainnet:test": "graph deploy  --product hosted-service  aminlatifi/giveth-economy subgraph.mainnet.yaml",
    "deploy:xdai:test": "graph deploy  --product hosted-service  aminlatifi/giveth-economy-xdai subgraph.xdai.yaml"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@graphprotocol/graph-cli": "0.25.0",
    "@graphprotocol/graph-ts": "0.25.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@types/node": "^16.4.1",
    "eslint": "^7.9.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.0.2"
  }
}
