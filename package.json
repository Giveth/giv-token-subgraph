{
  "name": "GivEconomy",
  "version": "0.1.1",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --max-warnings 0 . --ext .ts",
    "codegen:kovan": "graph codegen subgraph.kovan.yaml",
    "codegen:xdai": "graph codegen subgraph.xdai.yaml",
    "codegen": "yarn codegen:xdai && yarn codegen:kovan",
    "build:kovan": "graph build subgraph.kovan.yaml",
    "build:xdai": "graph build subgraph.xdai.yaml",
    "build": "yarn build:xdai && yarn build:kovan",
    "deploy:kovan": "graph deploy --product hosted-service mohammadranjbarz/giv-economy-kovan subgraph.kovan.yaml",
    "deploy:xdai": "graph deploy --product hosted-service mohammadranjbarz/giv-economy-xdai subgraph.xdai.yaml",
    "deploy:kovan:develop": "graph deploy --product hosted-service mohammadranjbarz/giv-economy-kovan-develop subgraph.kovan.yaml",
    "deploy:xdai:develop": "graph deploy --product hosted-service mohammadranjbarz/giv-economy-xdai-develop subgraph.xdai.yaml",
    "deploy": "yarn deploy:xdai && yarn deploy:kovan",
    "deploy:develop": "yarn deploy:xdai:develop && yarn deploy:kovan:develop",
    "create-local": "graph create --node http://localhost:8020/ mohammadranjbarz/gardenUnipool",
    "remove-local": "graph remove --node http://localhost:8020/ mohammadranjbarz/gardenUnipool",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 mohammadranjbarz/gardenUnipool"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@graphprotocol/graph-cli": "0.23.1",
    "@graphprotocol/graph-ts": "0.23.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@types/node": "^16.4.1",
    "eslint": "^7.9.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.0.2"
  }
}
